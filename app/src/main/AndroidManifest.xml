<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.applications.androidApplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.applications.androidApplication.SeventhActivity"></activity>
        <activity android:name="com.applications.androidApplication.SixthActivity" />
        <activity android:name="com.applications.androidApplication.FifthActivity" />
        <activity android:name="com.applications.androidApplication.FourthActivity" />
        <activity android:name="com.applications.androidApplication.ThirdActivity" />
        <activity android:name="com.applications.androidApplication.SecondActivity" />
        <activity android:name="com.applications.androidApplication.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>